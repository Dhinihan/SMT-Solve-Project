v: ARRAY [0..6] OF BOOLEAN;
n: ARRAY [0..6] OF BOOLEAN;
V: INT -> BOOLEAN = LAMBDA(i: INT): v[i];
N: INT -> BOOLEAN = LAMBDA(i: INT): n[i];
VN: (INT,INT) -> REAL = LAMBDA(i,j: INT): IF V(i) AND N(j) THEN i+j ELSE 0 ENDIF;
ASSERT FORALL(i: INT): v[i] <=> NOT n[i];
ASSERT FORALL(i: INT): n[i] <=> NOT v[i];
ASSERT v[0] AND  v[1] AND v[3] AND v[6] = TRUE;
ASSERT n[2] AND  n[4] AND n[5] AND n[7] = TRUE;
ASSERT FORALL(i: INT): i < 0 OR i > 7 => NOT v[i];
ASSERT FORALL(i: INT): i < 0 OR i > 7 => NOT n[i];
CHECKSAT EXISTS(i,j: INT): i >= 0 AND j >= 0 AND VN(i,j) > 0;
